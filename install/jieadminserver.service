[Unit]
Description=JieadminServer daemon
After=network.target

[Service]
User=root
Group=root
WorkingDirectory=/opt/jieserver/jieadminpanel/
ExecStart=/opt/jieserver/pyenv/versions/jieadminpanel3119/bin/gunicorn -b 0.0.0.0:8000 --env DJANGO_SETTINGS_MODULE=conf.settings --chdir  '/opt/jieserver/jieadminpanel/' --reload -n 'JieAdminPanelServer' -t 1800 conf.wsgi
Restart=always

[Install]
WantedBy=multi-user.target